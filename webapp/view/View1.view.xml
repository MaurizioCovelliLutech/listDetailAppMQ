<mvc:View controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <App>
        <Page
            title="{i18n>titleList}">
            <List
                items="{modCarrier>/}"
                itemPress="onListItemPress">
                <ObjectListItem
                    title="{modCarrier>Carrid}"
                    number=""
                    type="Active">
                    <ObjectAttribute text="{modCarrier>Carrname}" />
                </ObjectListItem>
                   
            </List>
        </Page>
    </App>
</mvc:View>
