<mvc:View controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <App>
        <Page
            title="{i18n>titleList}">
            <List
                items="{
                    path: '/UX_C_Carrier_TP'
                }"
                itemPress="onListItemPress">
                <ObjectListItem
                    title="{Carrname}"
                    number="{Carrid}"
                    type="Active"/>
            </List>
        </Page>
    </App>
</mvc:View>
